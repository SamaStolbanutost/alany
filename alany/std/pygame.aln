class PyGame() {
    def init() {
        pygame init;
    }

    def quit() {
        pygame quit;
    }

    class Display() {
        def init() {
            pygame display init;
        }

        def set_mode(width, height) {
            pygame display set_mode width height;
        }

        def set_caption(caption) {
            pygame display set_caption caption;
        }
    }
    var display = Display;
}

class Event() { 
    var type = 0;
}
