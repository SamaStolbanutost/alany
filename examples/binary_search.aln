var a = input;
convert int a;

var min = 1;
var max = 100;
var b = 50;
repeat 100 {
    if (a == b) {
        print 'Ready' \n;
        break;
    }
    if (a != b) {
        if (b > a) {
            var max = b;
            print 'Min:' min \n;
            print 'Max:' max \n;
        }
        if (b < a) {
            var min = b;
            print 'Min:' min \n;
            print 'Max:' max \n;
        }
        add b max min;
        div b b 2;
    }
}