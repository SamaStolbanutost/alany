var str a = input;
convert int a;

var float min = 1;
var float max = 100;
var float b = 50;
repeat 100 {
    if (a == b) {
        print 'Ready' \n;
        break;
    }
    if (a != b) {
        if (b > a) {
            var float max = b;
            print 'Min:' min \n;
            print 'Max:' max \n;
        }
        if (b < a) {
            var float min = b;
            print 'Min:' min \n;
            print 'Max:' max \n;
        }
        add b max min;
        div b b 2;
    }
}